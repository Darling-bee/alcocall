{
  "name": "alcocall",
  "version": "1.0.0",
  "description": "Virtual Bartender",
  "main": "server.js",
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "materialize-css": "^1.0.0",
    "mongoose": "^5.10.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-materialize": "^3.9.3",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amaliawhay/alcocall.git"
  },
  "author": "Amalia Hay, Ian Martinez, Yulin Karaiscos, Brianne Tengwall",
  "license": "",
  "bugs": {
    "url": "https://github.com/amaliawhay/alcocall/issues"
  },
  "homepage": "https://github.com/amaliawhay/alcocall#readme"
}
